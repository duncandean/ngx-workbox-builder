{
  "$schema": "http://json-schema.org/schema",
  "id": "WorkboxBuilderAddSchematic",
  "title": "Workbox Builder Add Schematic Options Schema",
  "type": "object",
  "properties": {
    "project": {
      "type": "string",
      "description": "The name of the project.",
      "$default": {
        "$source": "projectName"
      }
    },
    "target": {
      "type": "string",
      "description": "The target to apply Workbox builder to.",
      "default": "build"
    },
    "configuration": {
      "type": "string",
      "description": "The configuration to apply Workbox builder to.",
      "default": "production"
    }
  },
  "required": []
}
